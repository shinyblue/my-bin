#!/bin/bash
# removes lines from one file found in another
# removelines file_with_lines_to_remove file_to_alter
echo "Lines in $2 which do not occur in $1 :"
tmp1=~/removelinestemp1
tmp2=~/removelinestemp2
rm -f "$tmp1" "$tmp2"
cp "$2" "$tmp2"
while read l 
do	
	if [[ ${#l}>0 ]]
	then 	fgrep -v "$l" <"$tmp2" >"$tmp1"
			mv "$tmp1" "$tmp2" -f
	fi
done <"$1"
cat $tmp2
rm -f "$tmp1" "$tmp2"

