#!/bin/bash
# uses ImageMagick to create A4 size pages with 4 pictures on
# each one is roughly 6x4 inches 
if [ "${#}" -lt "2" ] 
then
	echo "only ${#} arguments given"
	echo "${@}"
	echo "Usage: "
	echo "${0} <imagefile> [<imagefile>...] <outputfile>"
else 
	montage -geometry "1689x1181+24+24>" -tile 2x2 "${@}"
fi
